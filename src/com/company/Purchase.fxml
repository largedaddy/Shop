<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.Purchase">
   <children>
      <ComboBox fx:id="ProductPurchase" layoutX="226.0" layoutY="88.0" onAction="#ProductPlay" prefWidth="150.0" promptText="Инструмент" />
      <ComboBox fx:id="ClientPurchase" layoutX="227.0" layoutY="155.0" onAction="#ClientPlay" prefWidth="150.0" promptText="Клиент" />
      <TextField fx:id="PricePurchase" layoutX="227.0" layoutY="213.0" promptText="Цена" />
      <Button fx:id="ButtonPurchase" layoutX="267.0" layoutY="279.0" mnemonicParsing="false" onAction="#PurchasePlay" text="Добавить" />
      <Button fx:id="BackPurchase" layoutX="24.0" layoutY="347.0" mnemonicParsing="false" onAction="#BackPlay3" text="Назад" />
      <Label layoutX="240.0" layoutY="27.0" text="ОформлениеПокупки" />
   </children>
</AnchorPane>
