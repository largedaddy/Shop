<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.Product">
   <children>
      <ComboBox fx:id="TypeProduct" layoutX="244.0" layoutY="36.0" onAction="#TypePlay" prefWidth="150.0" promptText="Тип" />
      <TextField fx:id="NameProduct" layoutX="245.0" layoutY="89.0" promptText="Наименование" />
      <TextField fx:id="CompanyProduct" layoutX="245.0" layoutY="138.0" promptText="Фирма" />
      <TextField fx:id="ModelProduct" layoutX="245.0" layoutY="188.0" promptText="Модель" />
      <TextField fx:id="PriceProduct" layoutX="245.0" layoutY="240.0" promptText="Цена" />
      <TextField fx:id="QuantityProduct" layoutX="245.0" layoutY="288.0" promptText="Количество" />
      <Button fx:id="ButtonProduct" layoutX="285.0" layoutY="336.0" mnemonicParsing="false" onAction="#ProductPlay" text="Добавить" />
      <Button fx:id="BackProduct" layoutX="40.0" layoutY="349.0" mnemonicParsing="false" onAction="#BackPlay2" text="Назад" />
